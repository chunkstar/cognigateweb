{
  "name": "cognigate",
  "version": "1.0.0",
  "description": "AI gateway with budget controls, local fallback, and voice mode - never overspend on AI again",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./voice": {
      "import": "./dist/voice/voice-chat.mjs",
      "require": "./dist/voice/voice-chat.cjs",
      "types": "./dist/voice/voice-chat.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.cjs",
      "types": "./dist/api/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "openai",
    "anthropic",
    "gateway",
    "budget",
    "cost-control",
    "ollama",
    "local-ai",
    "voice",
    "typescript"
  ],
  "author": "Cognigate Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkstar/cognigateweb.git"
  },
  "bugs": {
    "url": "https://github.com/chunkstar/cognigateweb/issues"
  },
  "homepage": "https://cognigate.dev",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
